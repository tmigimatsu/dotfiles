{
	"title": "Change home and end keys",
	"rules": [
		{
			"description": "Change home key to command+left",
			"manipulators": [
				{
					"type": "basic",
					"from": {
						"key_code": "home"
					},
					"to": [
						{
							"key_code": "left_arrow",
							"modifiers": [
								"left_command"
							]
						}
					]
				}
			]
		},
		{
			"description": "Change end key to command+right",
			"manipulators": [
				{
					"type": "basic",
					"from": {
						"key_code": "end"
					},
					"to": [
						{
							"key_code": "right_arrow",
							"modifiers": [
								"left_command"
							]
						}
					]
				}
			]
		}
	]
}
